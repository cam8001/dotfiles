! ===[Total Hackiness]=============================
! OS X does not support #define nor #include blocks in .Xdefaults.
! Therefore these are all overrides from the OS X side.
#include ".Xdefaults"

! ===[Xft Configuration]===========================
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

! ===[Font Configuration]==========================
urxvt.font: xft:Mensch:pixelsize=11
urxvt.boldFont: xft:Mensch:pixelsize=11
urxvt.italicFont: xft:Mensch:pixelsize=11:style=italic
urxvt.boldItalicFont: xft:Mensch:pixelsize=11:style=italic

! ===[Extensions]==================================
urxvt.perl-lib:          /home/smerrill/.urxvt/urxvt-perls:/home/smerrill/.urxvt/urxvt-tabbedex
urxvt.keysym.M-c:        perl:clipboard:copy
urxvt.keysym.C-T:        perl:tabbedex:new_tab
urxvt.keysym.C-Next:     perl:tabbedex:next_tab
urxvt.keysym.C-Prior:    perl:tabbedex:prev_tab
urxvt.urlLauncher:       /usr/bin/chromium

! Back to normal Linux copypasta.
URxvt.copyCommand:  xsel -ib
URxvt.pasteCommand: xsel -ob

